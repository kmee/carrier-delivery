<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      <record model="ir.ui.view" id="view_shipment_response_form">
          <field name="name">shipping.response.form</field>
          <field name="model">shipping.response</field>
          <field name="arch" type="xml">
            <form string="Romaneio" version="7.0">
              <header>
                <button name="shipment_confirm"
                    string="Confirm"
                    states="draft"
                    class="oe_highlight"
                    type="object"/>
                <button name="button_action_transit"
                    string="Sent to transit"
                    states="confirmed"
                    class="oe_highlight"
                    type="object"/>
                <button name="cancel"
                    string="Cancel"
                    states="draft,confirmed"/>
                <button name="action_cancel_draft"
                    states="cancel"
                    string="Set to Draft"
                    type="object"/>
                <field name="state" select="1" readonly="1"
                    widget="statusbar"
                    statusbar_visible="draft,confirmed,done" />
              </header>
              <sheet string="Romaneio">
                <div class="oe_title">
                  <h1>
                    <label string="Shipment Plan" />
                    <field name="name" class="oe_inline" attrs="{'invisible': [('state', '=', 'draft')]}"/>
                  </h1>
                </div>
                <group>
                  <group>
                    <field name="user_id"/>
                    <field name="partner_id"/>
                    <field name="carrier_id"/>
                    <field name="carrier_responsible" />
                    <field name="date"/>
                  </group>
                </group>
                <notebook>
                  <page string="Details">
                    <group>
                      <group>
                          <field name="carrier_tracking_ref"/>
                       </group>
                   </group>
                    <group>
                      <group>
                        <field name="volume"/>
                      </group>
                      <group>
                        <field name="weight"/>
                        <field name="weight_net"/>
                      </group>
                    </group>
                    <div class="oe_clear">
                      <label for="note"/>
                    </div>
                    <field name="note" colspan="4" class="oe_inline" placeholder="Additional notes..."/>
                  </page>
                  <page string="Entregas">
                    <field name="picking_line"/>
                      <!--<tree>-->
                        <!--<field name="product_id"/>-->
                        <!--<field name="state"/>-->
                        <!--&lt;!&ndash;<field name="product_uom_qty"/>&ndash;&gt;-->
                        <!--&lt;!&ndash;<field name="product_uom"  string="Unit of Measure" groups="product.group_uom"/>&ndash;&gt;-->
                        <!--&lt;!&ndash;<field name="location_dest_id"/>&ndash;&gt;-->
                        <!--&lt;!&ndash;<field name="string_availability_info"/>&ndash;&gt;-->
                        <!--&lt;!&ndash;<button name="action_remove_from_shipment"&ndash;&gt;-->
                            <!--&lt;!&ndash;type="object"&ndash;&gt;-->
                            <!--&lt;!&ndash;string="Remove from the shipment."&ndash;&gt;-->
                            <!--&lt;!&ndash;icon="gtk-cancel"&ndash;&gt;-->
                            <!--&lt;!&ndash;attrs="{'invisible': [('state', '=', 'done')]}"&ndash;&gt;-->
                            <!--&lt;!&ndash;/>&ndash;&gt;-->
                      <!--</tree>-->
                    <!--</field>-->
                  </page>
                </notebook>
              </sheet>
              <!--<div class="oe_chatter">-->
                <!--<field name="message_follower_ids" widget="mail_followers"/>-->
                <!--<field name="message_ids" widget="mail_thread"/>-->
              <!--</div>-->
            </form>
          </field>
        </record>

